{
  "name": "fullstackopenbackend",
  "version": "1.0.0",
  "description": "Ejercicios del bootcamp fullstackopen",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "build:ui": "rm -rf build && cd ../FullStackOpen/gia-telefonica && npm run build --prod && cp -r build ../../FullStackOpenBackend/",
    "deploy": "git push heroku $(git branch --show-current):main",
    "deploy:full": "npm run build:ui && git add . && git commit -m uibuild && npm run deploy",
    "logs:prod": "heroku logs --tail",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "David Scoffield",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "4.17.1",
    "mongoose": "^5.11.15",
    "mongoose-unique-validator": "^2.0.3"
  },
  "devDependencies": {
    "nodemon": "2.0.7"
  }
}
